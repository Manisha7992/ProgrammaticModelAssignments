<apex:page standardController="Bike_Detail__c" extensions="BikeDetailsExtension">
  <apex:form >
    <apex:pageBlock title="BikeDetails" mode="edit">
    <apex:pageBlockSection title="My Bike Details" columns="1">
        <apex:inputField value="{!Bike_Detail__c.Category__c}"/>
        <apex:inputField value="{!Bike_Detail__c.Colour_Variants__c}"/>
        <apex:inputField value="{!Bike_Detail__c.Fuel_Tank_Capacity__c}"/>
        <apex:inputField value="{!Bike_Detail__c.Fuel_Type__c}"/>
        <apex:inputField value="{!Bike_Detail__c.Maximum_Speed__c}"/>
        <apex:inputField value="{!Bike_Detail__c.Tyre_Type__c}"/>
    </apex:pageBlockSection>
    <apex:pageBlockButtons >
        <apex:commandButton value="save" action="{!save}"/>
    </apex:pageBlockButtons>
   </apex:pageBlock>
  </apex:form>
 </apex:page>